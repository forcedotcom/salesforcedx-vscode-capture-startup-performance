{
  "name": "salesforcedx-vscode-capture-startup-performance",
  "displayName": "Capture Extension Startup Performance",
  "description": "Extension that captures the Extension Activation Stats from 'Developer: Startup Performance' and reports the data to Azure AppInsights",
  "version": "1.0.0",
  "publisher": "salesforce",
  "repository": {
    "url": "https://github.com/forcedotcom/salesforcedx-vscode-capture-startup-performance"
  },
  "icon": "images/VSCodeApex.png",
  "license": "BSD-3-Clause",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/src/index.js",
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@types/jest": "^29.5.12",
    "@types/vscode": "^1.86.0",
    "@tsconfig/node18": "^18.2.4",
    "eslint": "^8.57.0",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "prettier": "^3.3.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "esbuild": "^0.23.1"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile && npm run bundle:extension && npm run update-for-packaging",
    "compile": "tsc --build",
    "compile:clean": "tsc --build --clean",
    "reinstall": "git checkout -- package-lock.json && git clean -xfd && npm install",
    "watch": "tsc --build --watch",
    "prettier": "prettier --write .",
    "prettier-check": "prettier --check .",
    "lint": "eslint . --ext .ts",
    "update-snapshots": "jest --updateSnapshot",
    "test": "jest --coverage",
    "test-vsix": "ts-node scripts/installVSIXFromBranch.ts code",
    "test-vsix-insiders": "ts-node scripts/installVSIXFromBranch.ts code-insiders",
    "bundle:extension": "esbuild ./src/index.ts  --bundle --outfile=dist/index.js --format=cjs --platform=node --external:vscode --minify",
    "update-for-packaging": "ts-node scripts/updatePackageJsonForVSIX.ts"
  },
  "extensionDependencies": ["salesforce.salesforcedx-vscode-core"],
  "activationEvents": [
    "workspaceContains:sfdx-project.json"
  ],
  "contributes": {
    "menus": {
      "commandPalette": [
          {
            "command": "sf.capture.startup.performance"
          }
      ]
    },
    "commands": [
      {
        "command": "sf.capture.startup.performance",
        "title": "%capture_startup_performance_command_text%"
      }
    ]
  }
}
